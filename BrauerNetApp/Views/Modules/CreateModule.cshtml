@model BrauerNetApp.Models.Module

@{
    ViewData["Title"] = "Module - Display";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("CreateModule", "Modules"))
{
    @Html.LabelFor(module => module.Title)
    @Html.EditorFor(module => module.Title)

    @Html.HiddenFor(module => module.QUESTORId)
    @Html.HiddenFor(module => module.ModuleId)

    <input type="submit" value="Create" class="btn btn-success" />
}
